# Depends on:  

pkgname=man-pages
pkgver=5.11
pkgrel=1
arch=('x86_64')
source=(https://www.kernel.org/pub/linux/docs/$pkgname/$pkgname-$pkgver.tar.xz)

build() {
	echo "build"
}

package() {
        cd $pkgname-$pkgver
        make DESTDIR=$pkgdir install
}
