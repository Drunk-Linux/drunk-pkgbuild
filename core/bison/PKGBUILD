# Description: A parser generator
# URL:         
# Maintainer:  Emmett1, emmett1 dot 2miligrams at gmail dot com
# Depends on:  

pkgname=bison
pkgver=3.7.6
pkgrel=1
arch=('x86_64')
source=(https://ftp.gnu.org/gnu/$pkgname/$pkgname-$pkgver.tar.xz)

build() {
        cd $pkgname-$pkgver

        ./configure --prefix=/usr
	make -j1
}

package() {
	cd $pkgname-$pkgver
        make -j1 DESTDIR=$pkgdir install
}
