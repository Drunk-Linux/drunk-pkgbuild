# Description: Utilities to administer Access Control Lists, which are used to define more fine-grained discretionary access rights for files and directories
# URL:         http://savannah.nongnu.org/projects/acl
# Maintainer:  Emmett1, emmett1 dot 2miligrams at gmail dot com
# Depends on:  

pkgname=markupsafe
pkgver=2.0.1
pkgrel=1
arch=('x86_64')
depends=('python3')
source=(https://files.pythonhosted.org/packages/source/M/MarkupSafe/MarkupSafe-$pkgver.tar.gz)

build() {
        cd MarkupSafe-$pkgver

	python3 setup.py build
}

package() {
	cd MarkupSafe-$pkgver
	python3 setup.py install --prefix=$pkgdir/usr --optimize=1
}
