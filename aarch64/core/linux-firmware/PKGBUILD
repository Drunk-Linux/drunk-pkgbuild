pkgname=linux-firmware
pkgver=20210511
pkgrel=1
arch=('aarch64')
options=(!strip)
source=(https://git.kernel.org/pub/scm/linux/kernel/git/firmware/linux-firmware.git/snapshot/linux-firmware-$pkgver.tar.gz)

build() {
	echo "nothing"
}

package() {
        cd $pkgname-$pkgver
        make DESTDIR=$pkgdir install
}
